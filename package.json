{
  "name": "airel-client",
  "version": "0.0.1",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "svelte-kit sync && npm run gen",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "gen": "graphql-codegen --config ./.graphqlrc.yaml",
    "gen:dev": "graphql-codegen --config ./.graphqlrc_dev.yaml"
  },
  "devDependencies": {
    "@algolia/autocomplete-js": "^1.7.1",
    "@algolia/autocomplete-theme-classic": "^1.7.1",
    "@bytemd/plugin-gfm": "^1.13.1",
    "@cloudinary/html": "^1.2.0",
    "@cloudinary/url-gen": "^1.7.0",
    "@kahi-ui/framework": "^0.6.3",
    "@kitql/all-in": "^0.6.3",
    "@splidejs/svelte-splide": "^0.2.5",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.73",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@zerodevx/svelte-toast": "^0.7.1",
    "algoliasearch": "^4.13.1",
    "bytemd": "^1.13.1",
    "cookie": "^0.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "github-markdown-css": "^5.1.0",
    "graphql": "^16.3.0",
    "instantsearch.js": "^4.42.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "quill": "^1.3.7",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module"
}
